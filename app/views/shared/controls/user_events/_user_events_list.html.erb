<div class='container'>
<% Event.all.each do |event| %>
<div class='row border'>
  <div class='col-md-2'>
    <%= event.date.to_time.strftime('%B %e') %><br/>
  </div>
  <div class='col-md-2'>
     starts: <%= event.start_time.strftime("%I:%M%p") %><br/>
    ends: <%= event.start_time.strftime("%I:%M%p") %><br/>
  </div>
  <div class='col-md-2'>
    <%= event.city.name %> <br/>
    </div>
  <div class='col-md-3'>
    <%= event.title %><br>
    <%= event.description %><br>
    
  </div>
  <div class='col-md-3'>
    Cost: <%= event.cost %><br/>
    Capacity: <%= event.capacity %>
    
  </div>
</div>
<% end %>
</div>